{
  "name": "frontend-plugin-example",
  "version": "3.0.0",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "vite build",
    "lint": "eslint .",
    "msgmerge": "for i in src/i18n/*.po; do msgmerge -Us --previous --backup=none \"$i\" dist/example.pot; done",
    "serve": "vite preview",
    "test": "echo \"No tests to run\""
  },
  "devDependencies": {
    "@open-xchange/lint": "^0.2.0",
    "@open-xchange/rollup-plugin-po2json": "^0.9.2",
    "@open-xchange/vite-plugin-ox-css": "^0.1.2",
    "@open-xchange/vite-plugin-ox-externals": "^0.8.0",
    "@open-xchange/vite-plugin-ox-manifests": "^0.8.1",
    "@open-xchange/vite-plugin-proxy": "^0.9.3",
    "dotenv": "^17.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "vite": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,mjs,cjs}": "eslint --cache --fix"
  },
  "resolutions": {
    "semver": "^7.5.2"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9",
        "eslint-plugin-n": "17"
      }
    }
  }
}
