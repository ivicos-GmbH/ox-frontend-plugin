{
  "name": "@open-xchange/appsuite-ui-e2e-tests-frontend-plugin-example",
  "version": "1.0.0",
  "description": "E2E Tests for App Suite UI Frontend Plugin Example",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "e2e": "pnpm codeceptjs run"
  },
  "dependencies": {
    "@open-xchange/appsuite-codeceptjs": "0.6.2",
    "dotenv": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "overrides": {
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "axios@>=1.3.2 <1.7.3": ">=1.7.4",
      "uuid": ">=9.0.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "chai": "5"
      }
    },
    "neverBuiltDependencies": [
      "detox",
      "edgedriver",
      "geckodriver",
      "pactum",
      "centra",
      "lightcookie",
      "phin"
    ],
    "ignoredOptionalDependencies": [
      "@codeceptjs/detox-helper"
    ],
    "updateConfig": {
      "ignoreDependencies": [
        "chalk"
      ]
    }
  }
}
