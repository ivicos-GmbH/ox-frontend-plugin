{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en, en-US, en-GB",
  "allowCompoundWords": true,
  "ignoreRegExpList": [
    "(className|iconClass|id|name|disp|prio): '.*'",
    "(format|import|point|trigger)\\('.*'\\)",
    "appSuite",
    "data-.*=\".*\"",
    "eslint (.*)",
    "html-attributes",
    "imports",
    "jira",
    "regexp",
    "paths",
    "comments-authors"
  ],
  "dictionaries": [
    "aws",
    "companies",
    "css",
    "filetypes",
    "fonts",
    "html",
    "lorem-ipsum",
    "misc",
    "node",
    "npm",
    "public-licenses",
    "softwareTerms",
    "typescript"
  ],
  "patterns": [
    {
      "name": "imports",
      "pattern": [
        " from '.*'",
        "import '.*'$"
      ]
    },
    {
      "name": "regexp",
      "pattern": [
        "\\.replace\\(\\/.*\\/",
        "\\/\\^(.*)\\$/",
        "\\/\\^.*/(i|g|m)",
        "\\/.*/\\.test",
        "RegExp\\(.*\\)"
      ]
    },
    {
      "name": "jquery-class-functions",
      "pattern": "(add|has|toggle)Class\\('.*'\\)?"
    },
    {
      "name": "appSuite",
      "pattern": [
        "\\/appsuite"
      ]
    },
    {
      "name": "jira",
      "pattern": [
        "OXUIB?-"
      ]
    },
    {
      "name": "html-attributes",
      "pattern": [
        "class=\".*\""
      ]
    },
    {
      "name": "paths",
      // pattern that matches all paths that start with a slash or a dot or an @
      "pattern": [
        "\"(\\/|\\.|@|node_modules|virtual).*\"",
        "'(\\/|\\.|@|node_modules|virtual).*'",
        "url\\(.*\\)"
      ]
    },
    {
      "name": "comments-authors",
      "pattern": [
        "// @author .*"
      ]
    }
  ],
  "overrides": [
    {
      "filename": "CHANGELOG.md",
      "ignoreRegExpList": [
        "\\[\\`[a-z1-9]+\\`\\]"
      ]
    }
  ],
  "ignorePaths": [
    "*.{eml,mp3,ogg,svg,tgz}",
    ".cspell.json",
    ".dockerignore",
    ".git",
    ".gitlab-ci.yml",
    "bundles.json",
